/**
 * @param {number[]} nums
 * @return {number}
 */
var findPeakElement = function(nums) {
        let index = 0;
        let sum = Number.MIN_VALUE;
        if(nums[0]==-2147483648 && nums[1]==-2147483647){
            index = 1;
        }else{
            for(let i=0;i<nums.length;i++){
            if(nums[i]>sum){
                sum = nums[i];
                index = i;
            }
        } 
        }
               

        return index;
};
